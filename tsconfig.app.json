{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "ESNext",
  "moduleResolution": "Bundler",
    "target": "ESNext",
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "types": ["vite/client"],
    "baseUrl": "./src"
  },
  "include": ["src"],
  "exclude": ["dist", "node_modules"]
}
